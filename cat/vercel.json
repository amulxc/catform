{
    "version": 2,
    "builds": [{
        "src": "cat/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "cat/wsgi.py"
        }
    ],
    "env": {
        "DJANGO_SETTINGS_MODULE": "cat.settings",
        "PYTHONUNBUFFERED": "1",
        "PYTHONPATH": "."
    }
}